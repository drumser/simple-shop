{
  "name": "quantick/simple-shop",
  "description": "simple shop task",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Alexander",
      "email": "mail@kladov.xyz"
    }
  ],
  "require": {
    "vlucas/phpdotenv": "^4.1",
    "symfony/http-foundation": "^5.0",
    "nikic/fast-route": "^1.3",
    "symfony/console": "^5.0",
    "php-di/php-di": "^6.0",
    "symfony/finder": "^5.0",
    "moneyphp/money": "^3.3",
    "doctrine/orm": "^2.7",
    "fzaninotto/faker": "^1.9",
    "ramsey/uuid-doctrine": "^1.6",
    "guzzlehttp/guzzle": "^6.5",
    "monolog/monolog": "^2.0",
    "ext-json": "*",
    "doctrine/migrations": "^2.0"
  },
  "autoload": {
    "files": [
      "./framework/functions.php"
    ],
    "psr-4": {
      "FMW\\": "framework/",
      "App\\": "src/"
    }
  },
  "scripts": {
    "test": [
      "vendor/bin/phpunit",
      "vendor/bin/psalm"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "vimeo/psalm": "^3.9"
  }
}
